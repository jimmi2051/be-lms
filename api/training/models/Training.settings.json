{
  "connection": "default",
  "collectionName": "training",
  "info": {
    "name": "training",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "numberOfCourse": {
      "type": "integer",
      "default": "0"
    },
    "isActive": {
      "type": "boolean"
    },
    "users": {
      "collection": "user",
      "via": "trainings",
      "plugin": "users-permissions"
    },
    "description": {
      "type": "text"
    },
    "numberOfStudent": {
      "type": "integer"
    },
    "level": {
      "type": "string"
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "categorytrainings": {
      "collection": "categorytraining",
      "via": "trainings"
    },
    "activityusers": {
      "collection": "activityuser",
      "via": "trainings"
    },
    "learningpaths": {
      "collection": "learningpath",
      "via": "training"
    }
  }
}