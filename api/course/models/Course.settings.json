{
  "connection": "default",
  "collectionName": "course",
  "info": {
    "name": "course",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "numberOfSection": {
      "type": "integer"
    },
    "isActive": {
      "type": "boolean"
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "users": {
      "collection": "user",
      "via": "courses",
      "dominant": true,
      "plugin": "users-permissions"
    },
    "learningpaths": {
      "collection": "learningpath",
      "via": "courses"
    },
    "relationcoursemodules": {
      "collection": "relationcoursemodule",
      "via": "courses"
    },
    "description": {
      "type": "text"
    }
  }
}